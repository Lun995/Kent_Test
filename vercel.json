{
  "version": 2,
  "routes": [
    {
      "src": "/(.*\\.(png|jpg|jpeg|gif|svg|ico|css|js|mp3|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
